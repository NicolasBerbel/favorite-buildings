{
  "name": "functions",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "serve": "concurrently \"npm run build -- --watch\" \"firebase emulators:start --only functions\"",
    "shell": "npm run build && firebase functions:shell",
